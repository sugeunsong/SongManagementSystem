public class Song {

	String name;
	String singer;
	String genre;
	static int numSongRegistered = 0;
	
 	public Song() {
 		numSongRegistered++;
 		
 	}
 	
 	public Song(String name, String singer, String genre) {
 		this.name = name;
 		this.singer = singer;
 		this.genre = genre;
 		numSongRegistered++;
 	}
 	
 	public void printInfo() {
 		System.out.println("name:" + this.name + " singer:" + this.singer + " genre:" + this.genre);
 	}
	
}
