public class Car {
	 private int year;
	 private String maker;
	 private double speed;
	 public void car() {
	  this.year = 0;
	  this.maker = "";
	  this.speed = 0.0;
	 }
	 public Car(int year, String maker, double speed) {
	  this.year = year;
	  this.maker = maker;
	  this.speed = speed;
	 }

	 public void change_speed(double speed)
	 {
	  this.speed = speed;
	 }

	 public void Print()
	 {
	  System.out.println("년식 : " + this.year);
	  System.out.println("메이커 : " + this.maker);
	  System.out.println("속도 : " + this.speed);
	 }
	}
