public class Calculate {

	public static void main(String[] args) {
		int[][] A = {
				{1, 0, 2}, 
				{-1, 3, 1}
		};
		int[][] B = {
				{3,1}, 
				{2, 1}, 
				{1, 0}
		};
		
		int [][] result = new int [2][2];
		
		for(int i=0; i<3; i++) {
		    for(int j=0; j<2; j++) {
		    	for(int k=0; k<3; k++) {
		    		result[i][j] += A[i][k]*B[k][j];
		    	}
		    	System.out.print(result[i][j]+" ");	
		    }
         }
	}
}
